{
  "name": "@acme/auth",
  "version": "2.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./env": "./src/env.ts",
    "./providers/*": "./src/providers/*.ts"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "clean:cache": "git clean -xdf .cache",
